{
  "name": "lekki-gathering-place-backend",
  "version": "1.0.0",
  "description": "Backend API for Lekki Gathering Place Skills Training Management System",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "rimraf dist && tsc && tsc-alias",
    "start": "node dist/server.js",
    "start:prod": "node dist/server.js",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:seed:classes": "ts-node prisma/seed-classes.ts",
    "prisma:seed:lessons": "ts-node prisma/seed-lessons-from-schedule.ts",
    "prisma:reset": "prisma migrate reset",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "church",
    "training",
    "management"
  ],
  "author": "",
  "license": "ISC",
